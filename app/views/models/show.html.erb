<div class="home-box-dashboard">
	<%= link_to "< Back to all schemas / models", main_app.dataset_file_schemas_path %>
	<div class="row">
		<div class="head-dataset">
			<div class="col-md-3">
				<h2><span class="fa fa-folder-open"></span><%= @model.name %></h2>
				<h3>Model: <%= @model %></h3>
				<h3>Schema Fields: <%= @model.model_schema_fields %></h3>
				<h3>Schema Field Constraints: <%= @model.model_schema_fields[0].model_schema_constraint %></h3>
			</div>
		</div>
	</div>
	<%= render(partial: "model_metadata", locals: { model: @model })  %>
</div>